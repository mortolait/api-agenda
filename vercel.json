{
    "name":"api-agenda",
    "version":2,
    "public":true,
    "builds": [
        {
            "src":"src/server.ts",
            "use":"@vercel/node"
        }
    ],
    "routes":[
        {
            "src":"/(.*)",
            "dest":"src/server.ts"
        },
        {
            "src":"/(.*)",
            "dest":"src/controller/appointmentController.ts"
        }
    ]
}