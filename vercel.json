{
    "name":"api-agenda",
    "version":2,
    "public":true,
    "builds": [
        {
            "src":"src/server.js",
            "use":"@vercel/node"
        }
    ],
    "routes":[
        {
            "src":"/(.*)",
            "dest":"src/server.js"
        },
        {
            "src":"/clientSaas/(.*)",
            "dest":"src/routes/clientSaasRoutes.js"
        }
    ]
}