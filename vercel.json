

{
    "name":"api-agenda",
    "version":2,
    "public":true,
    "builds": [
        {
            "src":"src/server.ts",
            "use":"@vercel/node"
        }
    ],
    "routes":[
        {
            "src":"/(.*)",
            "dest":"src/server.ts"
        },
       
        { "src": "/(.*)", "dest": "src/routes/clientSaasRoutes.ts" },
        { "src": "/(.*)", "dest": "src/routes/userRoutes.ts" }
    ]
    
}